{
	"name": "meridius/tester-extras",
	"type": "library",
	"description": "Collection of classes intended to ease testing with Nette/Tester based on Kdyby/TesterExtras",
	"keywords": ["nette", "database", "testing", "tester", "integration", "concurrent"],
	"homepage": "https://github.com/meridius/tester-extras",
	"license": "Apache-2.0",
	"authors": [
		{
			"name": "Martin Luke≈°",
			"homepage": "https://github.com/meridius/tester-extras"
		}
	],
	"support": {
		"issues": "https://github.com/meridius/tester-extras/issues"
	},
	"replace": {
		"meridius/database-tester": "*"
	},
	"require": {
		"nette/tester": "^1.6",
		"nette/di": "^2.3",
		"nette/bootstrap": "^2.3",
		"nette/robot-loader": "^2.3",
		"tracy/tracy": "^2.3",
		"nette/database": "v2.3.8",
		"nette/http": "^2.3"
	},
	"require-dev": {
		"jakub-onderka/php-parallel-lint": "^0.9.2",
		"jakub-onderka/php-console-highlighter": "^0.3.2",
		"squizlabs/php_codesniffer": "^2.5",
		"sebastian/phpcpd": "^2.0",
		"pdepend/pdepend": "^2.2",
		"phploc/phploc": "^3.0",
		"meridius/coding-standard": "*"
	},
	"autoload": {
		"psr-0": {
			"Meridius\\TesterExtras\\": "src/"
		}
	}
}
